  nav {
    height: 3rem;
    position: relative;
    width: 100%;
  }

  nav ul {
    height: 3rem;
    margin: 0;
    padding: 0;
    display: inline-block;
    font-size: 0;
    list-style: none;
  }

  nav ul li {
    font-size: 1rem;
    padding: 0;
    display: inline-block;
    margin: 5px;
  }



  nav ul.right {
    float: right;
  }

  button {
    height: 3rem;
    font-size: 1rem;
    padding: 3px;
    background-color: #4155E8;
    color: white;
    width: 100px;
    line-height: 1rem;
    
  }

  button:hover {
    cursor: pointer;
    background-color: #3B4CD1;
  }

  .center {
    float: left;
  }
